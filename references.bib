@article{Goswami:2025dwd,
    title = {Constraining a \$f({R}, {L}\_m)\$ {Gravity} {Cosmological} {Model} with {Observational} {Data}},
    url = {http://arxiv.org/abs/2505.18226},
    doi = {10.48550/arXiv.2505.18226},
    abstract = {We investigate a spatially flat FLRW cosmological model in the framework of modified gravity described by the function {\textbackslash}( f(R, L\_m) = αR + L\_m{\textasciicircum}β+ γ{\textbackslash}), where {\textbackslash}( L\_m {\textbackslash}) is the matter Lagrangian density. The modified Friedmann equations yield the Hubble parameter as \$ H(z) = H\_0 {\textbackslash}sqrt\{(1 - λ) + λ(1 + z){\textasciicircum}\{3(1 + w)\}\},\$ with the parameters {\textbackslash}( λ= {\textbackslash}fracγ\{6αH\_0{\textasciicircum}2\} + 1 {\textbackslash}) and {\textbackslash}( w = {\textbackslash}frac\{β(n - 2) + 1\}\{2β- 1\} {\textbackslash}). Using a Bayesian Markov Chain Monte Carlo (MCMC) approach, we constrain the model parameters with recent observational data, including cosmic chronometers, the Pantheon+ Supernovae dataset, Baryon Acoustic Oscillations (BAO), and Cosmic Microwave Background (CMB) shift parameters. The best-fit values are found to be {\textbackslash}( H\_0 = 72.773{\textasciicircum}\{+0.148\}\_\{-0.152\} {\textbackslash}) km/s/Mpc, {\textbackslash}( λ= 0.289{\textasciicircum}\{+0.007\}\_\{-0.007\} {\textbackslash}), and {\textbackslash}( w = -0.002{\textasciicircum}\{+0.002\}\_\{-0.002\} {\textbackslash}), all quoted at the 1{\textbackslash}(σ{\textbackslash}) confidence level.This model predicts a transition redshift of {\textbackslash}( z\_t {\textbackslash}approx 0.76 {\textbackslash}) for the onset of cosmic acceleration and an estimated universe age of 13.21 Gyr. The higher inferred value of {\textbackslash}( H\_0 {\textbackslash}) compared to the Planck 2018 result offers a potential resolution to the Hubble tension. Additionally, using {\textbackslash}( ρ\_0 = 0.534 {\textbackslash}times 10{\textasciicircum}\{-30\} {\textbackslash}, {\textbackslash}text\{g/cm\}{\textasciicircum}3 {\textbackslash}) and assuming {\textbackslash}( n = 1 {\textbackslash}), we derive the model constants as {\textbackslash}( β= 1.00201 {\textbackslash}), {\textbackslash}( α= 512247 {\textbackslash}), and {\textbackslash}( γ= -1.215 {\textbackslash}times 10{\textasciicircum}\{-29\} {\textbackslash}). We also evaluate the Bayesian Information Criterion (BIC) to compare the model's performance with that of the standard {\textbackslash}(Λ{\textbackslash})CDM model. The small BIC difference ({\textbackslash}( Δ{\textbackslash}text\{BIC\} = 0.16 {\textbackslash})) indicates comparable statistical support for both models. Thus, the {\textbackslash}( f(R, L\_m) {\textbackslash}) gravity scenario serves as a consistent and viable alternative to {\textbackslash}(Λ{\textbackslash})CDM, potentially addressing open questions in late-time cosmology.},
